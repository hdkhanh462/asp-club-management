<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - IctuTaekwondo</title>

	<link rel="stylesheet" href="~/css/site.output.css" asp-append-version="true" />

	<script src="https://cdn.jsdelivr.net/npm/flowbite@2.5.1/dist/flowbite.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/flowbite-datepicker@1.3.0/dist/js/datepicker-full.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.4/dist/js/locales/vi.js"></script>

</head>
<body>
	@await Html.PartialAsync("~/Views/Shared/Partials/_NavbarPartial.cshtml")

	<div class="p-4">
		<div class="mt-14">
			<main class="min-h-screen">
				@RenderBody()
			</main>
		</div>
	</div>
	@await Html.PartialAsync("~/Views/Shared/Partials/_FooterPartial.cshtml")

	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
